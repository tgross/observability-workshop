{
  "consul": "{{ .CONSUL }}:8500",
  "preStart": "python /usr/local/bin/manage.py",
  "logging": {
    "level": "INFO"
  },
  "services": [
    {
      "name": "mysql",
      "interfaces": ["net1", "eth0", "docker0"],
      "port": 3306,
      "health": "python /usr/local/bin/manage.py health",
      "poll": 5,
      "ttl": 25
    }
  ]
}
