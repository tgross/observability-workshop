{
  "consul": "127.0.0.1:8500",
  "preStart": ["/usr/local/bin/consul-manage", "preStart"],
  "services": [
    {
      "name": "consul",
      "interfaces": ["net1", "docker0"],
      "port": 8500,
      "health": ["curl --fail -s -o /dev/null localhost:8500/v1/status/leader"],
      "poll": 10,
      "ttl": 25
    }
  ]
}
